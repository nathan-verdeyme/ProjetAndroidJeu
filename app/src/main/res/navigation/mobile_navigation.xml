<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_deck">

    <fragment
        android:id="@+id/navigation_deck"
        android:name="com.example.projectjeu.ui.deck.DeckActivity"
        android:label="@string/title_deck"
        tools:layout="@layout/fragment_deck" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.projectjeu.ui.home.HomeActivity"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.projectjeu.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
</navigation>